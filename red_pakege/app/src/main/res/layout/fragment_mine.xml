    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/swiperefreshlayout_mine"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <include layout="@layout/layout_mine_head" />

        <include layout="@layout/layout_mine_content1" />

        <include layout="@layout/layout_mine_content2" />

        <include layout="@layout/layout_mine_content3" />

    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.swiperefreshlayout.widget.SwipeRefreshLayout>

